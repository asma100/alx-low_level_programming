#include "main.h"
11;rgb:0000/0000/0000/**
 * _pow_recursion - gives  x raised to the power of y
 * @x: a integer as input
 * @y:a integer as input
 * Return: x raised to the power of y
 */
int _pow_recursion(int x, int y)
{
if (y == 0)
return (1);
else if  (y < 0)
return (-1);
else
return (x * _pow_recursion(x, y - 1));
}
